<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}AdminPanel{% endblock %}</title>
        {% block stylesheets %}
        <link href="{{ asset('css/bootstrap.css') }}" rel="stylesheet" media="screen"/>
        <link href="{{ asset('bundles/zkadminpanel/js/bootstrap-datepicker/css/datepicker3.css') }}" rel="stylesheet" media="screen"/>
        <link href="{{ asset('bundles/zkadminpanel/css/admin-panel.css') }}" rel="stylesheet" media="screen"/>
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        {% block javascripts %}
        <script src="{{ asset('js/jquery.js') }}" type="text/javascript"></script>
        <script src="{{ asset('js/bootstrap.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/zkadminpanel/js/bootstrap-datepicker/js/bootstrap-datepicker.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/zkadminpanel/js/admin-panel-filter.js') }}" type="text/javascript"></script>
        {% endblock %}
    </head>
    <body style="background: #C0C1D3;">
        <div class="container">
            <h1>Admin Panel</h1>
            {% block body %}{% endblock %}
        </div>
    </body>
</html>
